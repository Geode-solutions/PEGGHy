{
  "name": "pegghy",
  "private": true,
  "type": "module",
  "scripts": {
    "clean_cache": "npx nuxi cleanup",
    "dev": "npm run clean_cache & npx nuxi dev",
    "dev:browser": "BROWSER=true npm run dev",
    "dev:desktop": "npm run dev",
    "build": "nuxi build --prerender",
    "generate": "nuxt generate",
    "compile_back": "node node_scripts/run_script.cjs compile back",
    "compile_viewer": "node node_scripts/run_script.cjs compile viewer",
    "compile_microservices": "npm run compile_back && npm run compile_viewer",
    "install_back": "node node_scripts/run_script.cjs install back",
    "install_viewer": "node node_scripts/run_script.cjs install viewer",
    "install_microservices": "npm run install_back && npm run install_viewer",
    "build_back": "node node_scripts/run_script.cjs back/build",
    "build_viewer": "node node_scripts/run_script.cjs viewer/build",
    "build_microservices": "npm run build_back && npm run build_viewer"
  },
  "dependencies": {
    "@geode/opengeodeweb-back": "5.8.7",
    "@geode/opengeodeweb-front": "9.8.2",
    "@geode/opengeodeweb-viewer": "1.10.1",
    "@geode/vease": "v1.12.0-rc.7",
    "@geode/vease-back": "1.4.15",
    "@geode/vease-viewer": "2.3.13",
    "@nuxt/devtools": "2.6.2",
    "@pinia/nuxt": "0.11.2",
    "@vueuse/nuxt": "13.5.0",
    "broadcast-channel": "7.1.0",
    "draggable": "4.2.0",
    "electron": "^36.4.0",
    "electron-updater": "6.6.2",
    "get-port-please": "3.1.2",
    "h3-formidable": "1.0.0",
    "nuxt": "3.17.7",
    "nuxt-electron": "^0.7.0",
    "pidtree": "^0.6.0",
    "vtk.js": "33.1.0",
    "vue": "3.5.17",
    "vue-router": "4.5.1",
    "vuetify": "^3.9.2",
    "vuetify-nuxt-module": "0.18.7"
  }
}