{
  "name": "pegghy",
  "private": true,
  "type": "module",
  "scripts": {
    "clean_cache": "npx nuxi cleanup",
    "dev": "npm run dev:browser",
    "dev:browser": "node node_scripts/browser.js dev:nuxt",
    "dev:desktop": "npm run dev:nuxt",
    "dev:nuxt": "npm run clean_cache & npx nuxi@3.11.0 dev --no-fork",
    "build": "nuxi build --prerender",
    "build:browser": "cross-env BROWSER=true npm run build",
    "preview:browser": "nuxi preview",
    "test:browser": "node node_scripts/browser.js preview:browser",
    "generate": "nuxt generate",
    "compile_back": "node node_scripts/run_script_file.js compile back",
    "compile_viewer": "node node_scripts/run_script_file.js compile viewer",
    "compile_microservices": "npm run compile_back && npm run compile_viewer",
    "install_back": "node node_scripts/run_script_file.js install back",
    "install_viewer": "node node_scripts/run_script_file.js install viewer",
    "install_microservices": "npm run install_back && npm run install_viewer",
    "build_back": "node node_scripts/run_script_file.js back/build",
    "build_viewer": "node node_scripts/run_script_file.js viewer/build",
    "build_microservices": "npm run build_back && npm run build_viewer",
    "unpacked": "npm run build && electron-builder --dir",
    "package": "npm run build && electron-builder build --publish=never",
    "test:e2e:browser": "npx playwright test --config ./tests/e2e/browser/playwright.config.js",
    "test:e2e:desktop": "npx playwright test --config ./tests/e2e/desktop/playwright.config.js"
  },
  "dependencies": {
    "@geode/opengeodeweb-back": "5.10.5",
    "@geode/opengeodeweb-front": "9.11.7",
    "@geode/opengeodeweb-viewer": "1.11.4",
    "@nuxt/devtools": "2.6.2",
    "@pinia/nuxt": "0.11.2",
    "@vueuse/nuxt": "13.5.0",
    "electron": "^38.2.2",
    "nuxt": "3.17.7",
    "nuxt-electron": "^0.7.0",
    "pidtree": "^0.6.0",
    "spark-md5": "^3.0.2",
    "vtk.js": "33.1.0",
    "vue": "3.5.17",
    "vue-router": "4.5.1",
    "vuetify": "3.8.12",
    "vuetify-nuxt-module": "0.18.7"
  }
}